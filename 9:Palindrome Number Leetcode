import java.util.*;
class Solution { 
    public boolean isPalindrome(int n) {
         if(n<0)
         return false;   
        int rev=0;
        int copy=n;
        while(n!=0)
        {
            int digit=n%10;
            rev=rev*10+digit;
            n=n/10;
            
        }
        if(rev==copy)
           return true;
        else
            return false;
              }
}

  
